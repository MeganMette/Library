package library;

public class Books extends Articles {
	private String genre;

	public Books() {
		// TODO Auto-generated constructor stub
	}

	public Books(int articleId, String author, int numberOfPages, String datePublished, String genre, String itemType) {
		super(articleId, author, numberOfPages, datePublished, itemType);
		this.genre = genre;
	}

	public String getGenre() {
		return genre;
	}

	public void setGenre(String genre) {
		this.genre = genre;
	}

}
