package library;

public class GovernmentDocuments extends Articles {
	private String department;

	public GovernmentDocuments() {
		// TODO Auto-generated constructor stub
	}

	public GovernmentDocuments(int articleId, String author, int numberOfPages, String datePublished, String department,
			String itemType) {
		super(articleId, author, numberOfPages, datePublished, itemType);
		this.department = department;
	}

	public String getDepartment() {
		return department;
	}

	public void setDepartment(String department) {
		this.department = department;
	}

}
